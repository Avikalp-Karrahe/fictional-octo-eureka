# Discord Bot Token - Get this from Discord Developer Portal
DISCORD_TOKEN=your_bot_token_here

# Channel ID for the intro channel where users post their first message
INTRO_CHANNEL_ID=your_intro_channel_id_here

# Role ID for the "Outer Circle" role to be assigned
OUTER_CIRCLE_ROLE_ID=your_outer_circle_role_id_here

# OpenAI API Key - Get this from OpenAI Platform (platform.openai.com)
OPENAI_API_KEY=your_openai_api_key_here